{
  "name": "devshare-site",
  "version": "0.2.0",
  "description": "Open source development platform.",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "travis": "better-npm-run travis",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "compile:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "build:*"
      }
    },
    "build:dev": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "build:*"
      }
    },
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "build:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "test:*"
      }
    },
    "travis": {
      "command": "babel-node build/travis",
      "env": {
        "DEBUG": "travis:*"
      }
    },
    "deploy": {
      "command": "babel-node build/deploy",
      "env": {
        "DEBUG": "deploy:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kypertech/devshare-site.git"
  },
  "author": "Prescott Prue <sprue.dev@gmail.com> (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.9",
    "classnames": "^2.2.5",
    "codemirror": "^5.19.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "firepad": "^1.4.0",
    "fs-extra": "^0.30.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.8.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.6.0",
    "koa-static": "^3.0.0",
    "lodash": "^4.16.2",
    "material-ui": "^0.15.4",
    "node-sass": "^3.7.0",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.9.0",
    "react": "^15.0.0",
    "react-avatar-editor": "^4.1.3",
    "react-dom": "^15.0.0",
    "react-dropzone": "^3.6.0",
    "react-google-button": "^0.1.0",
    "react-icons": "^2.2.1",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.0",
