{
  "name": "devshare-site",
  "version": "0.2.2",
  "description": "Open source code sharing and development platform.",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server build",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "config": "better-npm-run config",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "build:dev": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build:prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "webpack-dashboard -c cyan -m -t Brawndo -- node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*",
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "test:*"
      }
    },
    "deploy": {
      "command": "node ./build/deploy",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "config": {
      "command": "node build/app-config",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kypertech/devshare-site.git"
  },
  "author": "Prescott Prue <sprue.dev@gmail.com> (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "codemirror": "5.23.0",
    "firepad": "1.4.0",
    "lodash": "4.17.2",
    "material-ui": "0.16.0",
    "mobile-detect": "1.3.5",
    "normalize.css": "4.2.0",
    "raven-js": "3.11.0",
    "react": "15.3.2",
    "react-avatar-editor": "4.1.3",
    "react-dom": "15.3.2",
    "react-dropzone": "^3.6.0",
    "react-ga": "^2.1.2",
    "react-google-button": "0.1.0",
    "react-icons": "^2.2.1",
    "react-redux": "4.4.6",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-auth-wrapper": "1.0.0",
    "redux-devshare": "0.2.1",
    "redux-form": "6.4.3",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
