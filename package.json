{
  "name": "devshare-site",
  "version": "0.1.1",
  "main": "bin/server.js",
  "engines": {
    "node": ">=4.0.0 <5.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build-bundle && npm run build-server && npm run copy",
    "build-bundle": "webpack -p --stats --progress --config webpack-production.config",
    "build-server": "webpack -p --stats --progress --config webpack-server-production.config",
    "postinstall": "npm run build",
    "copy-assets": "cp -a assets build",
    "copy-stats": "node ./lib/copy-stats",
    "copy": "npm run copy-assets && npm run copy-stats",
    "clean": "rimraf build",
    "test": "mocha -R spec --compilers js:babel-core/register ./test/setup.js ./test/**/*.js",
    "start": "node bin/dev-server",
    "production": "node bin/server"
  },
  "keywords": [
    "webpack",
    "redux",
    "react",
    "react-router",
    "kyper",
    "matter",
    "tessellate"
  ],
  "license": "MIT",
  "dependencies": {
    "codemirror": "^5.12.0",
    "expose-loader": "^0.7.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "firebase": "^2.4.1",
    "firepad": "^1.3.0",
    "history": "1.13.0",
    "kyper-grout": "0.2.8",
    "lodash": "^4.3.0",
    "material-ui": "^0.14.4",
    "proxy-middleware": "^0.13.1",
    "re-base": "^1.5.1",
    "react": "^0.14.3",
    "react-avatar-editor": "^3.2.0",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.3.0",
    "react-modal": "^0.6.1",
    "react-redux": "^2.1.2",
    "react-router": "2.0.0-rc6",
    "react-router-redux": "^3.0.0",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.3.0",
    "redux-grout": "0.2.3",
    "redux-router": "1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "serve-static": "^1.10.0",
    "source-map-support": "^0.3.3",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-lodash": "^2.0.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.16.0",
    "eslint": "^1.4.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.2.3",
    "mocha": "^2.4.5",
    "node-sass": "^3.3.2",
    "react-hot-loader": "^1.2.8",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.1",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "transform-loader": "^0.2.3",
    "webpack-dev-server": "1.14.0"
  }
}
